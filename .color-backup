:root {
    --main-header-color:#1F2937;
    --main-intro-color:#1F2937;
    --main-footer-color:#1F2937;

    /* Header Items */
    --main-logo-color:#F9FAF8;
    --main-nav-link-color:#E5E7EB;

    /* Intro Items */
    --intro-title-color:#F9FAF8;
    --intro-subtext-color:#E5E7EB;

    /* Information Sections */
    --information-title-color:#1F2937;

    /* Quote Section */
    --aspiring-quote-background-color: #E5E7EB;
    --quote-color:#1F2937;

    /* Call to action section */
    --call-to-action-title-color: #F9FAF8;
    --call-to-action-subtext-color: #E5E7EB;
    --call-to-action-link-color:#F9FAF8;
    --button-border-color: white;

    /* Button Section */
    --button-color:#3882F6;

    /* Footer Section */
    --footer-text-color:#F9FAF8;

    /* Fallback color for unloaded images */
    --image-container-background-color: lightslategray;
}

